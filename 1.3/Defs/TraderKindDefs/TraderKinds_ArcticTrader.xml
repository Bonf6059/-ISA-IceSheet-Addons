<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- Caravan -->
    
<TraderKindDef>
    <defName>ISA_Caravan_ArcticTrader</defName>
    <label>arctic trader</label>
    <stockGenerators>
        
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>700~1000</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>MedicineHerbal</thingDef>
        <countRange>10~20</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>WoodLog</thingDef>
        <countRange>200~300</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>Chemfuel</thingDef>
        <countRange>-300~175</countRange>
      </li>

      <li Class="StockGenerator_SingleDef">
        <thingDef>Leather_Heavy</thingDef>
        <countRange>300~700</countRange>
      </li>
        
      <li Class="StockGenerator_SingleDef">
        <thingDef>Leather_Bluefur</thingDef>
        <countRange>300~700</countRange>
      </li>
        
      <li Class="StockGenerator_SingleDef">
        <thingDef>Leather_Bear</thingDef>
        <countRange>300~700</countRange>
      </li>
        
      <li Class="StockGenerator_SingleDef">
        <thingDef>Leather_Light</thingDef>
        <countRange>150~250</countRange>
      </li>
        
      <li Class="StockGenerator_SingleDef">
        <thingDef>Leather_Bird</thingDef>
        <countRange>200~400</countRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>ComponentIndustrial</thingDef>
        <countRange>5~10</countRange>
      </li>
      
      <li Class="StockGenerator_Category">
        <categoryDef>FoodRaw</categoryDef>
        <excludedCategories>
          <li>EggsFertilized</li>
        </excludedCategories>
        <thingDefCountRange>3~3</thingDefCountRange>
        <totalPriceRange>150~800</totalPriceRange>
      </li>
      
      <li Class="StockGenerator_SingleDef">
        <thingDef>ISA_Leather_Pinniped</thingDef>
        <countRange>-150~450</countRange>
      </li>
        
      <li Class="StockGenerator_Animals">
        <maxWildness>0.99</maxWildness>
        <checkTemperature>true</checkTemperature>
        <tradeTagsSell>
          <li>AnimalCommon</li>
          <li>AnimalUncommon</li>
          <li>AnimalFarm</li>
          <li>AnimalPet</li>
          <li>AnimalFighter</li>
        </tradeTagsSell>
        <kindCountRange>0~4</kindCountRange>
        <countRange>2~6</countRange>
      </li>
      
      <!-- Buying -->
      
      <li Class="StockGenerator_BuyExpensiveSimple" />
      <li Class="StockGenerator_Animals">
        <maxWildness>0.99</maxWildness> 
        <checkTemperature>true</checkTemperature>
        <tradeTagsBuy>
          <li>AnimalCommon</li>
          <li>AnimalUncommon</li>
          <li>AnimalFarm</li>
          <li>AnimalPet</li>
          <li>AnimalFighter</li>
        </tradeTagsBuy>
      </li>
                  
        
    </stockGenerators>
  </TraderKindDef>    
</Defs>
